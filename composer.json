{
	"name": "esperecyan/webidl",
	"description": "Provides the utility class for casting a given value in accordance with WebIDL (Web IDL) type to help with PHP type declarations. / WebIDL (Web IDL) の型に沿うように、与えられた値をキャストするユーティリティクラスを提供し、PHP の型宣言を補助します。",
	"keywords": ["W3C", "EvalError", "RangeError", "ReferenceError", "TypeError", "URIError"],
	"homepage": "https://github.com/esperecyan/webidl",
	"license": "MPL-2.0",
	"authors": [
		{
			"name": "100の人",
			"homepage": "https://twitter.com/esperecyan",
			"role": "Developer"
		}
	],
	"require": {
		"php": ">=5.4",
		"symfony/polyfill-mbstring": "^1.3.0",
		"symfony/polyfill-php70": "^1.3.0",
		"ext-spl": "*",
		"ext-dom": "*",
		"ext-pcre": "*",
		"ext-date": "*",
		"ext-reflection": "*"
	},
	"require-dev": {
		"phpunit/phpunit": "^5.7.14",
		"apigen/apigen": "^4.1.2",
		"ext-xml": "*",
		"ext-gmp": "*",
		"ext-simplexml": "*"
	},
	"autoload": {
		"psr-4": {
			"esperecyan\\webidl\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"esperecyan\\webidl\\": "tests/"
		}
	}
}
